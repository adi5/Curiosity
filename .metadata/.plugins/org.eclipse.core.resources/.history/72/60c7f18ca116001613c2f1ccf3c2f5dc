package curiosity.bean_utility;

public class Contact {

	private String email;
	private String altrEmail;
	private String mobile;
	private String altrMobile;
	private String landline;
	private String altrLandline;
	/**
	 * @return the email
	 */
	public String getEmail() {
		return email;
	}
	/**
	 * @param email the email to set
	 */
	public void setEmail(String email) {
		this.email = email;
	}
	/**
	 * @return the altrEmail
	 */
	public String getAltrEmail() {
		return altrEmail;
	}
	/**
	 * @param altrEmail the altrEmail to set
	 */
	public void setAltrEmail(String altrEmail) {
		this.altrEmail = altrEmail;
	}
	/**
	 * @return the mobile
	 */
	public String getMobile() {
		return mobile;
	}
	/**
	 * @param mobile the mobile to set
	 */
	public void setMobile(String mobile) {
		this.mobile = mobile;
	}
	/**
	 * @return the altrMobile
	 */
	public String getAltrMobile() {
		return altrMobile;
	}
	/**
	 * @param altrMobile the altrMobile to set
	 */
	public void setAltrMobile(String altrMobile) {
		this.altrMobile = altrMobile;
	}
	/**
	 * @return the landline
	 */
	public String getLandline() {
		return landline;
	}
	/**
	 * @param landline the landline to set
	 */
	public void setLandline(String landline) {
		this.landline = landline;
	}
	/**
	 * @return the altrLandline
	 */
	public String getAltrLandline() {
		return altrLandline;
	}
	/**
	 * @param altrLandline the altrLandline to set
	 */
	public void setAltrLandline(String altrLandline) {
		this.altrLandline = altrLandline;
	}
}
